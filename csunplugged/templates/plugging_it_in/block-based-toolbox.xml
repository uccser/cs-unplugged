<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
    <category name="Looks" categorystyle="looks_category">
        <block type="looks_say"></block>
        <block type="looks_say">
            <value name="value">
                <block type="values_string">
                    <field name="TEXT">abc</field>
                </block>
            </value>
        </block>
        <block type="looks_say">
            <value name="value">
                <block type="values_number">
                    <field name="NUM">123</field>
                </block>
            </value>
        </block>
        <block type="looks_say">
            <value name="value">
                <block type="values_boolean"></block>
            </value>
        </block>
    </category>
    <category name="Control" categorystyle="control_category">
        <block type="controls_repeat_num_times">
            <value name="num">
                <block type="values_number">
                    <field name="NUM">10</field>
                </block>
            </value>
        </block>
        <!-- Forever loop? -->
        <block type="controls_if_then"></block>
        <block type="controls_if_then_else"></block>
        <block type="controls_repeat_until"></block>
        <block type="controls_stop"></block>
    </category>
    <category name="Sensing" categorystyle="sensing_category">
        <block type="sensing_ask_and_wait_text">
            <value name="question">
                <block type="values_string">
                    <field name="TEXT">What's your name?</field>
                </block>
            </value>
        </block>
        <block type="sensing_ask_and_wait_number">
            <value name="question">
                <block type="values_string">
                    <field name="TEXT">What's your age?</field>
                </block>
            </value>
        </block>
    </category>
    <category name="Operators" categorystyle="operators_category">
        <block type="operators_add">
            <value name="x">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="y">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
        </block>
        <block type="operators_subtract">
            <value name="x">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="y">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
        </block>
        <block type="operators_multiply">
            <value name="x">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="y">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
        </block>
        <block type="operators_divide">
            <value name="x">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="y">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
        </block>
        <block type="operators_random_int">
            <value name="a">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="b">
                <block type="values_number">
                    <field name="NUM">100</field>
                </block>
            </value>
        </block>
        <block type="operators_greater_than"></block>
        <block type="operators_less_than"></block>
        <block type="operators_equality"></block>
        <block type="operators_and"></block>
        <block type="operators_or"></block>
        <block type="operators_not"></block>
        <block type="operators_join_string">
            <value name="a">
                <block type="values_string">
                    <field name="TEXT">apple </field>
                </block>
            </value>
            <value name="b">
                <block type="values_string">
                    <field name="TEXT">banana</field>
                </block>
            </value>
        </block>
        <block type="operators_letter_of">
            <value name="index">
                <block type="values_number">
                    <field name="NUM">1</field>
                </block>
            </value>
            <value name="word">
                <block type="values_string">
                    <field name="TEXT">apple</field>
                </block>
            </value>
        </block>
        <block type="operators_length_of">
            <value name="VALUE">
                <block type="values_string">
                    <field name="TEXT">abc</field>
                </block>
            </value>
        </block>
        <block type="operators_string_contains">
            <value name="word">
                <block type="values_string">
                    <field name="TEXT">apple</field>
                </block>
            </value>
            <value name="subword">
                <block type="values_string">
                    <field name="TEXT">a</field>
                </block>
            </value>
        </block>
        </block>
        <block type="operators_modulo">
            <value name="a">
                <block type="values_number">
                    <field name="NUM">-3</field>
                </block>
            </value>
            <value name="n">
                <block type="values_number">
                    <field name="NUM">2</field>
                </block>
            </value>
        </block>
        <block type="operators_round">
            <value name="a">
                <block type="values_number">
                    <field name="NUM">3.14</field>
                </block>
            </value>
        </block>
        <block type="operators_single">
            <field name="OP">ABS</field>
                <value name="NUM">
                    <block type="values_number">
                        <field name="NUM">9</field>
                    </block>
            </value>
        </block>
    </category>
    <category name="Variables" categorystyle="variable_category" custom="VARIABLE"></category>
    <sep></sep>
    <category name="Values" categorystyle="values_category">
        <block type="values_number">
            <field name="NUM">0</field>
        </block>
        <block type="values_string">
            <field name="TEXT">abc</field>
        </block>
        <block type="values_boolean"></block>
    </category>
</xml>